#include <stdio.h>

int main() {
    int arr[10];
    int soma = 0;

    // Coletando os valores do usuário
    printf("Digite 10 números inteiros:\n");
    for (int i = 0; i < 10; i++) {
        printf("Número %d: ", i + 1);
        scanf("%d", &arr[i]);
        soma += arr[i];  // Somando os elementos diretamente
    }

    // Exibindo a soma total
    printf("\nA soma dos elementos do array é: %d\n", soma);

    return 0;
}
